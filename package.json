    "globals": "^15.14.0",
    "happy-dom": "^16.2.9",
    "lefthook": "^1.10.1",
    "postcss": "^8.4.49",
    "prettier": "3.4.2",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
